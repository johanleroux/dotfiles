@import "colors.rasi"
@import "font.rasi"

* {
    background-color: @bg-col;
    text-color: @fg-col;
}

window {
    width: 325px;
    padding: 20px;
    border: 3px;
    border-color: @border-col;
    background-color: @bg-col;
    location: 0;
    x-offset: 0;
    y-offset: 0;
}

entry {
    expand: true;
    width: 150px;
    text-color: @fg-col2;
}
